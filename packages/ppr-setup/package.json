{
  "name": "ppr-setup",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun server",
    "build": "bun run build:client && bun run build:server",
    "build:client": "bunx --bun vite build",
    "build:server": "bunx --bun vite build --ssr",
    "serve": "NODE_ENV=production bun server",
    "debug": "bun --inspect-brk server"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-router": "^1.133.27",
    "@tanstack/react-router-ssr-query": "^1.133.27",
    "@tanstack/router-core": "^1.133.27",
    "@tanstack/router-plugin": "^1.133.27",
    "compression": "^1.7.5",
    "express": "^4.21.2",
    "get-port": "^7.1.0",
    "isbot": "^5.1.31",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@tanstack/react-router-devtools": "^1.133.27",
    "@types/bun": "^1.3.1",
    "@types/express": "^4.17.21",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.1",
    "@vitejs/plugin-react": "^4.3.4",
    "typescript": "^5.7.2",
    "vite": "^7.1.7"
  }
}
